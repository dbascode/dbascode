{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": "src",
        "includePattern": "\\.?js$",
        "excludePattern": "(node_modules/|docs/|__tests_/)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-typeof-plugin",
        "node_modules/jsdoc-export-default-interop/dist/index"
    ],
    "opts": {
        "encoding": "utf8",
        "recurse": true
    },
    "templates": {
    }
}
